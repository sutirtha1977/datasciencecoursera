combine <- function(idIndex, directory){
startI <- 1
endI <- length(idIndex)
binded <- data.frame("mean"=integer(0))
while(startI <= endI){
otherDataset <- read.csv(formatFileName(idIndex[startI],directory), header=TRUE)
binded <- rbind(binded,otherDataset)
startI<-startI+1
}
binded
}
